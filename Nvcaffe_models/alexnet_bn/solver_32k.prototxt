net: "models/alexnet_bn/train_val2_fp16.prototxt"

test_iter:      196        # 196 =50000/256 # 1562 = 50000/32
test_interval:  40         #1250   
test_initialization: false

display:   10
 
max_iter:  4000 	# 100 epochs 
iter_size: 4

#base_lr:  2             # B=1024
#base_lr:  10            # B=8K
base_lr:   32           # B=32K

#min_lr:  0.005

larc: true
larc_policy: "clip"
larc_eta: 0.001

rampup_lr: 1
rampup_interval: 400      # 10 epochs

lr_policy: "poly"
power: 2.

momentum: 0.9
weight_decay: 0.0005

snapshot: 500000
snapshot_prefix: "models/alexnet_bn/snapshots/alexnetbn_fp16"
snapshot_after_train: false

solver_mode: GPU
random_seed: 1
#solver_data_type: FLOAT16

